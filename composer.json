{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-curl": "*",
        "authorizenet/authorizenet": "2.0.0",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "inacho/php-credit-card-validator": "^1.0",
        "intervention/image": "^2.4",
        "laravel/framework": "5.8.*",
        "laravel/socialite": "^4.4",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "2.*",
        "mailchimp/mailchimp": "^2.0",
        "mcred/detect-credit-card-type": "^0.1.0",
        "php-http/guzzle6-adapter": "^2.0",
        "spatie/laravel-newsletter": "4.2.*",
        "spatie/laravel-sitemap": "^5.2",
        "stripe/stripe-php": "^6.30",
        "symfony/translation": "4.3.8",
        "tymon/jwt-auth": "^1.0",
        "unisharp/laravel-filemanager": "^2.1",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files" : [
            "app/Helper/Helper.php",
            "app/Helper/Images.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
